import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String s = sc.next();
        String goal = sc.next();
        sc.close();

        if (s.length() != goal.length()) {
            System.out.println("false");
        
        }else{

        String L = s + s;
        System.out.println(L.contains(goal));
        }
        }
}
